{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "types": [
      "node"
    ]
  },
  "include": [
    "src",
    "scripts",
    "ssg.config.ts",
    "vite.config.ts",
    "src/routeTree.gen.ts"
  ]
}